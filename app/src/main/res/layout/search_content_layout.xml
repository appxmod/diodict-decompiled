        <LinearLayout android:gravity="center" android:orientation="horizontal" android:id="@+id/changedPopupTopLayout2" android:paddingTop="5.3dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_alignLeft="@+id/changedPopupTopLayout" android:layout_alignTop="@+id/changedPopupTopLayout" android:layout_alignRight="@+id/changedPopupTopLayout" android:layout_alignBottom="@+id/changedPopupTopLayout" android:layout_alignParentTop="true">
