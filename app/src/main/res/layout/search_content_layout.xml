<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" android:id="@+id/SearchContentRelativeLayout" android:background="@drawable/search_content_bg" android:layout_width="match_parent" android:layout_height="match_parent">
