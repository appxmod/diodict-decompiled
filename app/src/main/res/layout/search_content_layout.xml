        </LinearLayout>
        <com.diotek.diodict.uitool.DictEditText android:id="@+id/SearchEditText" android:background="@drawable/search_text" android:paddingLeft="6.6dp" android:paddingRight="30dp" android:focusable="true" android:nextFocusRight="@+id/ClearBtn" android:clickable="true" android:longClickable="true" android:layout_width="180.6dp" android:layout_height="wrap_content" android:maxLines="1" android:lines="1" android:singleLine="true" android:maxLength="70" android:layout_toLeftOf="@+id/InputRightLayout" android:layout_toRightOf="@+id/InputLeftLayout" android:layout_centerVertical="true" android:privateImeOptions="inputType=dictionary" android:imeOptions="actionSearch"/>
        <ImageButton android:id="@+id/ClearBtn" android:background="@drawable/clearbtn" android:visibility="gone" android:nextFocusLeft="@+id/SearchEditText" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="6.6dp" android:layout_marginRight="1.5dp" android:layout_toLeftOf="@+id/InputRightLayout" android:layout_centerVertical="true"/>
